<h3>Edit <%= @project.name %></h3>

<%= form_for @project do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name, autofocus: true %>
  </div>
  <div>
    <%= f.label :desc %>
    <%= f.text_area :desc %>
  </div>
  <div>
    <%= f.label :img %>
    <%= f.text_field :img %>
  </div>
  <div>
    <%= f.label :hosted_url %>
    <%= f.text_field :hosted_url %>
  </div>
  <div>
    <%= f.label :github_url %>
    <%= f.text_field :github_url %>
  </div>
  <div>
    <%= f.label :active %>
    <%= f.check_box :active %>
  </div>
  <div>
    <%= f.submit "Update" %>
  </div>
<% end %>